    <string name="shell_description">A hard, rigid protective outer case of a mollusc</string>
    <string name="shelter_description">An external structure/housing made of twigs or sand particles</string>
    <string name="clearly_defined_legs_description">A well defined limb on which the invertebrate walks </string>
    <string name="segmented_body_description">A body that is devided into equal and equidistant parts</string>
    <string name="long_thin_body_description">An elongated body on which legs are attached</string>
    <string name="appendages_description">A projective part of the invertebrate with a distinct appearance</string>
    <string name="three_pairs_of_legs_description">Three limbs on each side of the body</string>
    <string name="four_or_more_pairs_of_legs_description">Four legs on each side of the body</string>
    <string name="elongated_tail_description">An elongated terminal appendage attached to the posterior end of the body/abdomen</string>
    <string name="tufted_tail_description">A tail with many small appendages attached at a local point resembling a small tuft</string>
    <string name="short_tail_description">A short terminal appendage attached to the posterior end of the body/abdomen</string>
    <string name="plate_like_gills_description">Gills with a plate-like appearance</string>
    <string name="feather_like_gills_description">Gills with a feather-like appearance</string>
    <string name="leaf_like_gills_description">Gills with a leaf-like appearance</string>
    <string name="bulging_eyes_description">Large protruding eyes forming part of the outline of the head</string>
    <string name="stocky_body_description">A short body on which legs are attached</string>
    <string name="antennae_description">A pair of short/long sensory appendages attached to the head</string>
    <string name="suckers_at_both_ends_description">Having sucking parts at both ends of the organisms by which they attach to a surface</string>
    <string name="wing_buds_description">Small flattened sturcutres on the upper body that will grow into wings</string>
    <string name="flattened_body_description">A flat and thin body</string>
    <string name="short_stubby_legs_description">Short stubby leg-like structures near the head on both sides of the body</string>
    <string name="rounded_body_description">A body which is rounded and has a smooth outline</string>

